#include <iostream>
#include <iomanip>

using namespace std;

int main() {
    double x = -4.0; 
    double shag = 0.5;

    cout << setw(10) << "x" << setw(15) << "y" << endl;

    while (x <= 4.0) {
        double y = 2 * x * x - 5 * x - 8;
        cout << setw(10) << fixed << setprecision(2) << x << setw(15) << fixed << setprecision(2) << y << endl;
        x += shag;
    }

    return 0;
}
