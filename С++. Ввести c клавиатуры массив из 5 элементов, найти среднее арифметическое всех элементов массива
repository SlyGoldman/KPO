#include <iostream>
using namespace std;

int main() {
    int array[5];
    int sum = 0;

    cout << "Введите 5 элементов массива: " << endl;
    for (int i = 0; i < 5; i++) {
        cin >> array[i];
        sum += array[i];
    }

    float srednee = sum / 5;

    cout << "Среднее арифметическое всех элементов массива: " << srednee << endl;

    return 0;
}
