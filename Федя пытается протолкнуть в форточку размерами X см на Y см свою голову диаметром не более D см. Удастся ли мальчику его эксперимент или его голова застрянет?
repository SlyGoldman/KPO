using System;

class Program
{
    static void Main()
    {
        double X = 20; 
        double Y = 30;
        double D = 25;

        bool uspeh = protolknet(X, Y, D);
        
        if (uspeh)
        {
            Console.WriteLine("Мальчику удастся протолкнуть голову через форточку");
        }
        else
        {
            Console.WriteLine("Мальчику не удастся протолкнуть голову через форточку");
        }
    }
    
    static bool protolknet(double X, double Y, double D)
    {
        return X >= D || Y >= D;
    }
}
