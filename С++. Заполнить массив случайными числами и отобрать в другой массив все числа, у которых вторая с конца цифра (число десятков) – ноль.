#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main() {
const int razmer = 10;
int arr[razmer];
int vibor[razmer];
int vibor_scetcik = 0;

srand(time(0));


for (int i = 0; i < razmer; i++) {
arr[i] = rand() % 100; 
}


for (int i = 0; i < razmer; i++) {
if ((arr[i] / 10) % 10 == 0) { 
vibor[vibor_scetcik] = arr[i];
vibor_scetcik++;
}
}


cout << "Исходный массив: ";
for (int i = 0; i < razmer; i++) {
cout << arr[i] << " ";
}
cout << endl;


cout << "Чисел с нулевой цифрой десятков: ";
for (int i = 0; i < vibor_scetcik; i++) {
cout << vibor[i] << " ";
}
cout << endl;

return 0;
}
