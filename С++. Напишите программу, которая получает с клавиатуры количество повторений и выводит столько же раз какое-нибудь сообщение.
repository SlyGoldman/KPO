#include <iostream>
#include <string>
using namespace std;

int main() {
    int povtor;
    string soobshenie;

    cout << "Введите количество повторений: ";
    cin >> povtor;

    cout << "Введите сообщение для повторений: ";
    cin.ignore();
    getline(cin, soobshenie);

    for (int i = 0; i < povtor; i++) {
        cout << soobshenie << endl;
    }

    return 0;
}
