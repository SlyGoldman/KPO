#include <iostream>

using namespace std;

int main() {
    const int razmer = 8;
    

    for (int i = 0; i < razmer; ++i) {
        for (int j = 0; j < razmer; ++j) {
            
            if ((i + j) % 2 == 0) {
                cout << "*";
            } else {
                cout << " ";
            }
        }
        cout << endl;
    }

    return 0;
}
