#include <iostream>
using namespace std;

int main() {
    int a, b;
    cout << "Введите два числа А и В через пробел так, чтобы выполнялось условие (А <= В): ";
    cin >> a >> b;

    int sum = 0;
    for (int i = a; i <= b; i++) {
        sum += i * i;
    }

    cout << "Сумма квадратов чисел от " << a << " до " << b << " будет " << sum << endl;

    return 0;
}
